
  {
    "version": 2,
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
    "builds": [{
      "src": "./index.js",
      "use": "@now/node-server"
    }],
    "routes": [{
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "index.js"
      }
    ]
  }